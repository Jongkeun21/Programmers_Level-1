    "def solution(x) :\n",
    "    answer = True\n",
    "    sum_ = 0\n",
    "    num = x\n",
    "\n",
    "    while x > 0 :\n",
    "        sum_ += x%10\n",
    "        x = x//10\n",
    "\n",
    "    if num%sum_ != 0 :\n",
    "        answer = False\n",
    "    \n",
    "    return answer"
   ]
  }
