    "def solution(n) :\n",
    "    dic = {num: True for num in range(2, n+1)}\n",
    "        \n",
    "    for i in range(2, int(n**0.5)+1) :\n",
    "        if dic[i] :\n",
    "            for j in range(i+i, n+1, i) :\n",
    "                dic[j] = False  \n",
    "    \n",
    "    return len([answer for answer in dic.values() if answer])"
   ]
